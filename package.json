{
   "name": "format-fast",
   "version": "1.0.2",
   "description": "format-fast is a function that allows you to replace certain words to make a text string dynamically, and very easily !",
   "main": "index.js",
   "type": "index.d.ts",
   "scripts": {
      "build": "yarn prebuild && tsc",
      "start": "node index.js",
      "dev": "yarn prebuild && tsc -w",
      "dev:start": "nodemon index.js",
      "prebuild": "rimraf index.js && rimraf index.d.ts"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/yoicalsin/format-fast.git"
   },
   "keywords": [
      "format",
      "string-replace",
      "replaces",
      "string",
      "format-fast"
   ],
   "author": "Yoni Calsin <helloyonicb@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/yoicalsin/format-fast/issues"
   },
   "homepage": "https://github.com/yoicalsin/format-fast#readme",
   "devDependencies": {
      "nodemon": "^2.0.3",
      "prettier-plugin-packagejson": "^2.2.3",
      "rimraf": "^3.0.2"
   },
   "dependencies": {
      "is-all-utils": "^1.0.6"
   }
}
